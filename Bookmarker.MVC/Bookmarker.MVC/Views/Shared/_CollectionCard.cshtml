@model Bookmarker.MVC.Models.CollectionViewModel

<div class="col-sm-4">
    <div class="card">
        <div class="card-header">
            <h5>@Model.Name</h5>
            <span class="glyphicon glyphicon-thumbs-up"></span>
        </div>
        <div class="card-body">
            <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
            <a href="#" class="btn btn-primary"><span class="glyphicon glyphicon-thumbs-up"></span>Go somewhere</a>
        </div>
        <div class="card-footer">
            <div class="card-text">
                <span class="glyphicon glyphicon-thumbs-up"></span>9001
                <span class="glyphicon glyphicon-user"></span>
            </div>
        </div>
    </div>
</div>